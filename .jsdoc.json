{
  "source": {
    "include": ["src"],
    "includePattern": "\\.ts$"
  },
  "tags": {
    "allowUnknownTags": ["optional"]
  },
  "plugins": ["node_modules/better-docs/typescript"],
  "opts": {
    "destination": "./docs/build",
    "recurse": true,
    "readme": "README.md",
    "template": "./node_modules/minami"
  }
}
